message TitleBasics {
    required binary tconst (STRING);
    required binary titleType (STRING);
    required binary primaryTitle (STRING);
    optional binary originalTitle (STRING);
    optional binary isAdult (STRING);
    optional binary startYear (STRING);
    optional binary endYear (STRING);
    optional binary runtimeMinutes (STRING);
    required group genres (LIST) {
        repeated binary array (STRING);
    }
}